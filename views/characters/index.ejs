<%- include("../partials/header")%>
<div class="text-center">
	<h1>Characters</h1>
</div>
	<div class="container">
		<div class="row">
			<%characters.forEach(function(character){%>
			<div class="col-6">
				<div class="row">
					<div class="col-4">
						<h1 class="charHead"><a href="/characters/<%character._id%>"><%=character.name%></a></h1>
						<img class="img-thumbnail CharThumb" src=<%=character.image%>>
					</div>
					<div class="col-8">
						<h4 class="charBioHead">
							Character Bio
						</h4>
					</div>
				</div>
			</div>
			<%})%>
		</div>
	</div>
